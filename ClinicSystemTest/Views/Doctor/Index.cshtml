@model IEnumerable<ClinicSystemTest.Models.ListDoctorAppointment>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>List Appointment</h1>


<table class="table">
    <tbody>
        @foreach (var item in Model)
        {
            if (item.IsAppointment)
            {
                <tr>
                    <td>
                        <a asp-action="OtherUserProfile" asp-route-id="@item.Id">@item.Date</a>
                        @*                @Html.DisplayFor(modelItem => item.Email)
                *@
                    </td>
                </tr>
            }

        }
    </tbody>
</table>
